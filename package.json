{
	"name": "zettel",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"lint": "node ./scripts/lint.mjs",
		"clean": "node ./scripts/clean.js",
		"clean-all": "node ./scripts/clean-all.js",
		"cleanall": "npm run clean-all",
		"install": "node ./scripts/install.mjs",
